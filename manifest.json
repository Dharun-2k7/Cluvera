{
    "manifest_version": 3,
    "name":"Cluvera - Ethical CP Hint Assistant",
    "version" : "1.0",
    "permissions": ["scripting", "activeTab", "storage"],
    "action":{
        "default_popup":"popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts":[
        {
            "matches": ["*://*.leetcode.com/*", "*://*.codeforces.com/*"],
            "js": ["content.js"]
        }
    ],
     
}